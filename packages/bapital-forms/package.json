{
  "name": "bapital-forms",
  "version": "1.1.0",
  "description": "Capital Framework form fields",
  "less": "src/bapital-forms.less",
  "style": "bapital-forms.css",
  "scripts": {
    "build": "yarn run compile && yarn run compress",
    "compile": "lessc src/bapital-forms.less bapital-forms.css --autoprefix='last 2 versions','Explorer >= 8'",
    "compress": "uglifycss bapital-forms.css > bapital-core.min.css"
  },
  "dependencies": {
    "bapital-buttons": "^1.1.0",
    "bapital-core": "^1.1.0",
    "bapital-grid": "^1.1.0",
    "bapital-icons": "^1.1.0"
  },
  "keywords": [
    "forms"
  ],
  "devDependencies": {
    "less": "^3.8.1",
    "less-plugin-autoprefix": "^2.0.0",
    "uglifycss": "^0.0.29"
  }
}
