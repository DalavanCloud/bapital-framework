{
  "name": "bapital-expandables",
  "version": "1.0.1",
  "description": "Capital Framework expandables",
  "main": "src/bapital-expandables.js",
  "less": "src/bapital-expandables.less",
  "style": "bapital-expandables.css",
  "scripts": {
    "build": "yarn run compile && yarn run compress",
    "compile": "lessc src/bapital-expandables.less bapital-expandables.css --autoprefix='last 2 versions','Explorer >= 8'",
    "compress": "uglifycss bapital-expandables.css > bapital-expandables.min.css"
  },
  "dependencies": {
    "bapital-atomic-component": "^1.0.1",
    "bapital-core": "^1.0.1",
    "bapital-icons": "^1.0.1",
    "classlist-polyfill": "1.0.3"
  },
  "keywords": [
    "expandable"
  ],
  "devDependencies": {
    "less": "^3.8.1",
    "less-plugin-autoprefix": "^2.0.0",
    "uglifycss": "^0.0.29"
  }
}
